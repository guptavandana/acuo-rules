package com.acuo.rules.eligibility

import org.slf4j.Logger

global Logger log;

rule "rule-moody-haircut-cmpt"
	when
		$provider:HaircutProvider(name=="Moody")
		$eligible:Eligible($vp: valuationPercentage!=-1)
	then
		$eligible.setIsEligible(true);
		$eligible.setFxHaircut(0);
		$eligible.setHaircut(1- $vp);
		update($eligible);
		System.out.println("fire rule-moody-haircut-cmpt");

end

