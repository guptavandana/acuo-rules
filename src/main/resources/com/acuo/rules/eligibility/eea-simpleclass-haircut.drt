template header
ruleName
classType
haircut

package com.acuo.rules.eligibility;

template "eea-simpleclass-haircut"

rule "@{ruleName}"
    when
        $eligible: Eligible(classType == "@{classType}")

    then
        $eligible.setHaircut(@{haircut});
        update($eligible);
        log.info("Rule: '{}',eligibility: '{}'", drools.getRule().getName(),$eligible);
end
end template
